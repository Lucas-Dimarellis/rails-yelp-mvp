<div class="container mt-5">
  <h1>Restaurants</h1>

  <ul id="restaurants">
    <% @restaurants.each do |restaurant| %>
      <li>
         <%= link_to restaurant.name, restaurant_path(restaurant) %>
      </li>
    <% end %>
  </ul>

    <%= link_to new_restaurant_path do %>
    <i class="fa-solid fa-arrow-rotate-left"></i> create new restaurant
  <% end %>
</div>
